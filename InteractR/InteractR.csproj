<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Version>1.0.1</Version>
    <PackageProjectUrl>https://github.com/madebykrol/InteractR</PackageProjectUrl>
    <RepositoryUrl>https://github.com/madebykrol/InteractR</RepositoryUrl>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Use case interactor.</Description>
    <Authors>Kristoffer Olsson</Authors>
    <Company>Madebykrol</Company>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CodeMetricsConfig.txt" />
  </ItemGroup>

  <ItemGroup>
    <AdditionalFiles Include="CodeMetricsConfig.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
